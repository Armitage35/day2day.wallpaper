{
	"short_name": "Day2Day.wallpaper",
	"name": "Day2Day.wallpaper",
	"version": "0.1",
	"manifest_version": 2,
	"icons": {
		"16": "favicon.ico",
		"48": "favicon.ico",
		"128": "favicon.ico"
	},
	"permissions": [
		"wallpaper",
		"https://www.google-analytics.com/",
		"storage"
	],
	"start_url": "./index.html",
	"display": "standalone",
	"theme_color": "#000000",
	"background_color": "#ffffff",
	"browser_action": {
		"default_popup": "index.html",
		"default_title": "Day2Day.wallpaper"
	},
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	}

}
